{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "project"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "propertyImg": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "propertyAddress": {
      "type": "string"
    },
    "propertyPrice": {
      "type": "integer"
    },
    "beds": {
      "type": "decimal"
    },
    "baths": {
      "type": "decimal"
    },
    "sqft": {
      "type": "integer"
    },
    "propertyType": {
      "type": "enumeration",
      "enum": [
        "Single family",
        "Multi family"
      ]
    },
    "yearBuilt": {
      "type": "integer"
    },
    "actualRent": {
      "type": "integer"
    },
    "projectedRent": {
      "type": "integer"
    },
    "latitude": {
      "type": "float"
    },
    "longitude": {
      "type": "float"
    },
    "propertyDesc": {
      "type": "text"
    },
    "occupancy": {
      "type": "enumeration",
      "enum": [
        "Has tenants",
        "Vacant"
      ]
    },
    "listingStatus": {
      "type": "enumeration",
      "enum": [
        "Available",
        "Under contract",
        "Sold"
      ]
    }
  }
}
